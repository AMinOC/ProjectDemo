<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- ExpandableLayout -->
    <declare-styleable name="ExpandableLayout">
        <attr name="parentLayout" format="integer"/>
        <attr name="childLayout" format="integer"/>
    </declare-styleable>

    <declare-styleable name="OldThumbUpView">
        <attr name="o_tuv_count" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CountView">
        <attr name="cv_count" format="integer"/>
        <attr name="cv_text_color" format="color"/>
        <attr name="cv_text_size" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="CenterFlowLayout">
        <!--相当于xml的marginLeft-->
        <attr name="childSpacing" format="dimension"/>
        <!--相当于xml的marginTop-->
        <attr name="rowSpacing" format="dimension"/>
    </declare-styleable>

</resources>